{
  "name": "roll-anything",
  "version": "4.0.1",
  "description": "A very tiny dice util. You can roll a dice with any amount of sides.",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "vitest run",
    "lint": "tsc --noEmit && eslint ./src",
    "build": "del-cli dist && tsc",
    "release": "npm run lint && npm run build && np"
  },
  "devDependencies": {
    "@cycraft/eslint": "^0.4.3",
    "@cycraft/tsconfig": "^0.1.2",
    "del-cli": "^6.0.0",
    "np": "^10.2.0",
    "vitest": "^3.0.6"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "roll",
    "dice",
    "roll-a-dice",
    "chance",
    "fifty-fitfy",
    "flip-a-coin"
  ],
  "author": "Luca Ban - Mesqueeb (https://cycraft.co)",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mesqueeb/roll-anything.git"
  },
  "homepage": "https://github.com/mesqueeb/roll-anything#readme",
  "bugs": "https://github.com/mesqueeb/roll-anything/issues"
}
